<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../../partials/head.ejs', {title: title}); %>
    <!-- <link rel="stylesheet" href="/css/bootstrap.min.css" /> -->
</head>

<body class="authentication-bg pb-0" data-layout-config='{"darkMode":false}'>
<div class="auth-fluid">
    <!--Auth fluid left content -->
    <div class="auth-fluid-form-box">
        <div class="align-items-center d-flex h-100" style="width: 100%">
            <div class="card-body">
                <!-- Logo -->
                <div class="m-auto auth-brand text-center text-lg-left">
                    <a href="/auth/login" class="logo-dark">
                        <span><img src="/images/logo-dark.png" alt="" height="18"/></span>
                    </a>
                    <a href="/auth/login" class="logo-light">
                        <span><img src="/images/logo.png" alt="" height="18"/></span>
                    </a>
                </div>

                <!-- title-->
                <h4 class="mt-0">Resend password</h4>
                <p class="text-muted m-0">Click the link in your email to change your password if it's your first time
                    to login</p>
                <span>or</span>
                <p class="text-muted mb-4">You have to contact the administrator to resend active link</p>

            </div>
            <!-- end .card-body -->
        </div>
        <!-- end .align-items-center.d-flex.h-100-->
    </div>
    <!-- end auth-fluid-form-box-->
</div>
<!-- end auth-fluid-->

<!-- bundle -->
<script src="/js/vendor.min.js"></script>
<script src="/js/app.min.js"></script>
</body>
</html>
