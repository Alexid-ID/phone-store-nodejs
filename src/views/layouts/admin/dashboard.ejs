<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../../partials/head.ejs', {title: title}); %>
</head>

<body
        class="loading"
        data-layout-config='{"leftSideBarTheme":"dark","layoutBoxed":false, "leftSidebarCondensed":false, "leftSidebarScrollable":false,"darkMode":false, "showRightSidebarOnStart": true}'
>
<!-- Begin page -->
<div class="wrapper">
    <!-- ========== Left Sidebar Start ========== -->
    <%- include('../../partials/left_sidebar.ejs') %>
    <!-- Left Sidebar End -->

    <!-- ============================================================== -->
    <!-- Start Page Content here -->
    <!-- ============================================================== -->

    <div class="content-page">
        <div class="content">
            <!-- Topbar Start -->
            <%- include('../../partials/topbar.ejs', {currentUser: user }) %>
            <!-- end Topbar -->

            <!-- Start Content-->
            <div class="container-fluid">
                <!-- start page title -->
                <div class="row">
                    <div class="col-12">
                        <div class="page-title-box">
                            <div class="page-title-right">
                                <div class="form-inline" id="filterTimeForm">
                                    <button class="filter-item btn btn-outline-primary ml-2 active" id="filterToday">
                                        <i class="mdi mdi-calendar-account"></i> Today
                                    </button>
                                    <button class="filter-item btn btn-outline-primary ml-2" id="filterYesterday">
                                        <i class="mdi mdi-calendar-clock"></i> Yesterday
                                    </button>
                                    <button class="filter-item btn btn-outline-primary ml-1" id="filterWeek">
                                        <i class="mdi mdi-calendar-check"></i> 7 days
                                    </button>
                                    <button class="filter-item btn btn-outline-primary ml-1" id="filterMonth">
                                        <i class="mdi mdi-calendar-month"></i> Month
                                    </button>
                                    <div class="ml-4 d-flex">
                                        <!-- from to -->
                                        <div class="form-group">
                                            <label for="fromDate">From</label>
                                            <input class="mx-1 form-control" id="fromDate" type="date" name="fromDate"
                                                   value="<%= new Date().toISOString().split('T')[0]; %>">
                                        </div>
                                        <div class=" form-group">
                                            <label for="toDate">To</label>
                                            <input class="mx-1 form-control" id="toDate" type="date" name="toDate"
                                                   value="<%= new Date().toISOString().split('T')[0]; %>">
                                        </div>
                                        <button class="filter-item btn btn-outline-primary ml-1" id="filterInRange">
                                            <i class="mdi mdi-filter"></i> Filter
                                        </button>
                                    </div>

                                </div>
                            </div>
                            <h4 class="page-title">Dashboard</h4>
                        </div>
                    </div>
                </div>
                <!-- end page title -->

                <div class="row">
                    <div class="col-12">
                        <div class="row">
                            <div class="col-lg-3 col-6">
                                <div class="card widget-flat">
                                    <div class="card-body">
                                        <div class="float-right">
                                            <i class="mdi mdi-account-multiple widget-icon"></i>
                                        </div>
                                        <h5
                                                class="text-muted font-weight-normal mt-0"
                                                title="Number of Customers"
                                        >
                                            Customers
                                        </h5>
                                        <h3 class="mt-3 mb-3" id="numCustomer">36,254</h3>
                                        <p class="mb-0 text-muted">
													<span class="text-success mr-2"
                                                    ><i class="mdi mdi-account-multiple-plus"></i> Total Customer </span
                                                    >
                                            <span class="text-nowrap">
                                                checkouts
                                        </span>
                                        </p>
                                    </div>
                                    <!-- end card-body-->
                                </div>
                                <!-- end card-->
                            </div>
                            <!-- end col-->

                            <div class="col-lg-3 col-6">
                                <div class="card widget-flat">
                                    <div class="card-body">
                                        <div class="float-right">
                                            <i class="mdi mdi-cart-plus widget-icon"></i>
                                        </div>
                                        <h5 class="text-muted font-weight-normal mt-0" title="Number of Orders">
                                            Orders
                                        </h5>
                                        <h3 class="mt-3 mb-3" id="numOrder">5,543</h3>
                                        <p class="mb-0 text-muted">
													<span class="text-danger mr-2"
                                                    ><i class="mdi mdi-cart-arrow-down"></i> Total Order</span
                                                    >
                                            <span class="text-nowrap">
                                            have been made
                                        </span>
                                        </p>
                                    </div>
                                    <!-- end card-body-->
                                </div>
                                <!-- end card-->
                            </div>

                            <div class="col-lg-3 col-6">
                                <div class="card widget-flat">
                                    <div class="card-body">
                                        <div class="float-right">
                                            <i class="mdi mdi-currency-usd widget-icon"></i>
                                        </div>
                                        <h5 class="text-muted font-weight-normal mt-0" title="Average Revenue">
                                            Product
                                        </h5>
                                        <h3 class="mt-3 mb-3" id="numProduct">$6,254</h3>
                                        <p class="mb-0 text-muted">
													<span class="text-danger mr-2"
                                                    ><i class="mdi mdi-cellphone-iphone"></i>
                                                    Total sold product</span>
                                        </p>
                                    </div>
                                    <!-- end card-body-->
                                </div>
                                <!-- end card-->
                            </div>
                            <!-- end col-->


                            <div class="col-lg-3 col-6">
                                <div class="card widget-flat">
                                    <div class="card-body">
                                        <div class="float-right">
                                            <i class="mdi mdi-currency-usd widget-icon"></i>
                                        </div>
                                        <h5 class="text-muted font-weight-normal mt-0" title="Average Revenue">
                                            Revenue
                                        </h5>
                                        <h3 class="mt-3 mb-3" id="numRevenue">$6,254</h3>
                                        <p class="mb-0 text-muted">
													<span class="text-danger mr-2"
                                                    ><i class="mdi mdi-coin"></i> Total revenue</span
                                                    >
                                            <span class="text-nowrap">
                                            of Phone Store
                                        </span>
                                        </p>
                                    </div>
                                    <!-- end card-body-->
                                </div>
                                <!-- end card-->
                            </div>
                            <!-- end col-->
                        </div>
                        <!-- end row -->
                    </div>
                    <!-- end col -->
                </div>
                <!-- end row -->


                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                <h4 class="header-title mt-2">Invoice List</h4>

                                <div class="table-responsive">
                                    <table class="table table-centered table-nowrap table-hover mb-0" id="invoiceTable">
                                        <tbody>

                                        </tbody>
                                    </table>
                                </div>
                                <!-- end table-responsive-->
                            </div>
                            <!-- end card-body-->
                        </div>
                        <!-- end card-->
                    </div>
                    <!-- end col-->

                </div>
                <!-- end row -->
            </div>
            <!-- container -->
        </div>
        <!-- content -->

        <!-- Footer Start -->
        <%- include('../../partials/footer.ejs') %>
        <!-- end Footer -->
    </div>

    <!-- ============================================================== -->
    <!-- End Page content -->
    <!-- ============================================================== -->
</div>
<!-- END wrapper -->

<!-- bundle -->
<script src="/js/vendor.min.js"></script>
<script src="/js/app.min.js"></script>

<!-- third party js -->
<script src="/js/vendor/apexcharts.min.js"></script>
<script src="/js/vendor/jquery-jvectormap-1.2.2.min.js"></script>
<script src="/js/vendor/jquery-jvectormap-world-mill-en.js"></script>
<!-- third party js ends -->

<!-- demo app -->
<!--<script src="/js/pages/demo.dashboard.js"></script>-->
<script src="/js/main/auth/logout.js"></script>
<script src="/js/main/admin/dashboard.js"></script>

<!-- end demo js-->
</body>
</html>
